{
  "name": "pubSub",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "pubSub/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/pubSub",
        "configFile": "pubSub/vite.config.ts"
      },
      "configurations": {
        "production": {
          "mode": "production"
        },
        "development": {
          "mode": "development"
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/vite:dev-server",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202",
        "configFile": "pubSub/vite.config.ts"
      },
      "configurations": {
        "production": {
          "buildTarget": "pubSub:build:production"
        },
        "development": {
          "buildTarget": "pubSub:build:development"
        }
      },
      "defaultConfiguration": "development",
      "dependsOn": ["shell:serve"]
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "pubSub:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "pubSub/jest.config.ts",
        "tsConfig": "pubSub/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "staticFilePath": "dist/pubSub",
        "port": 4202
      }
    }
  }
}
